---
permalink: api/library/insights.json
published: true
---
{% assign comma = false %}
{% assign posts = (site.insights) %}
[
  {% for post in posts %}
    {% unless post.hidden == true %}
      {% if post.url contains 'api/library/insights/' %}
          {% if comma %},{% endif %}
          {% assign comma = true %}
          {
            "id":"{{ post.id }}",
            "title":"{{ post.title }}",
            "url":"{{ post.url }}",
            "description":"{{ post.description }}",
            "download":"{{ post.download_source }}",
            "date":"{{ post.date }}",
            "download_source":"{{ post.download_source }}"
          }
      {% endif %}
    {% endunless %}
  {% endfor %}
]